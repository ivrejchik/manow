    "@hono/zod-validator": "^0.4.2",
    "@nats-io/jetstream": "^3.0.0",
    "@nats-io/transport-node": "^3.0.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@pdfme/common": "^5.3.3",
    "@pdfme/generator": "^5.3.3",
    "arctic": "^2.2.2",
    "argon2": "^0.41.1",
    "drizzle-orm": "^0.38.4",
    "hono": "^4.6.18",
    "ical-generator": "^8.0.0",
    "luxon": "^3.5.0",
    "postgres": "^3.4.5",
    "resend": "^4.0.1",
    "zod": "^3.24.1"
